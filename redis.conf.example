# Redis конфигурация для *{APP_NAME}

# Основные настройки
bind *{REDIS_HOST}
port *{REDIS_PORT}
timeout 0
tcp-keepalive 300

# Настройки безопасности
requirepass *{REDIS_PASSWORD}

# Настройки производительности
maxmemory 256mb
maxmemory-policy allkeys-lru

# Логирование
loglevel notice
logfile ""

# Сохранение данных
save 900 1
save 300 10
save 60 10000

# Настройки сети
tcp-backlog 511 